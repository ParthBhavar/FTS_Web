    <script>
            function ReplaceSpecialChar(strXml) {
        if (strXml != undefined) {
            strXml = strXml.replace(/&/g, '@@and;')
            strXml = strXml.replace(/</g, '@@LT;')
            strXml = strXml.replace(/>/g, '@@GT;')
            strXml = strXml.replace(/"/g, '@@DQ;')
            strXml = strXml.replace(/'/g, '@@SQ;')
            strXml = strXml.replace(/\//g, '@@PL;')
        }
        return strXml;  
    }

        </script>

